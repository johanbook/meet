{
  "name": "web-ui",
  "version": "0.0.0",
  "homepage": "https://meetly.site/",
  "scripts": {
    "build": "vite build && npm run build:service-worker",
    "build:service-worker": "workbox injectManifest workbox-config.js",
    "build:storybook": "storybook build",
    "format": "prettier --write '{scripts,src}/**/*.{js,jsx,ts,tsx}'",
    "lint": "tsc && eslint 'src/**/*.{js,jsx,ts,tsx}' --max-warnings 0",
    "lint:ci": "npm run lint",
    "start:dev": "vite --host --port 80",
    "start:storybook": "storybook dev -p 6006",
    "test": "vitest src",
    "test:ci": "npm run test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@mui/icons-material": "7.0.2",
    "@mui/material": "7.0.2",
    "@mui/x-data-grid": "8.1.0",
    "@mui/x-date-pickers": "8.1.0",
    "@react-spring/web": "9.7.5",
    "@sentry/react": "9.17.0",
    "@tanstack/react-query": "5.74.4",
    "@use-gesture/react": "10.3.1",
    "@vitejs/plugin-react": "^4.4.0",
    "dayjs": "1.11.13",
    "framer-motion": "12.9.1",
    "i18next": "25.0.1",
    "i18next-browser-languagedetector": "8.0.5",
    "i18next-http-backend": "3.0.2",
    "notistack": "3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "15.5.1",
    "react-router": "7.5.2",
    "react-swipeable-views": "0.14.0",
    "socket.io-client": "4.8.1",
    "typescript": "5.8.3",
    "uuid": "11.1.0",
    "vite": "6.3.4",
    "vite-tsconfig-paths": "5.1.4",
    "web-vitals": "4.2.4"
  },
  "devDependencies": {
    "@eslint/js": "9.27.0",
    "@storybook/addon-essentials": "8.6.12",
    "@storybook/addon-interactions": "8.6.12",
    "@storybook/addon-links": "8.6.12",
    "@storybook/blocks": "8.6.12",
    "@storybook/react": "8.6.12",
    "@storybook/react-vite": "8.6.14",
    "@storybook/test": "8.6.12",
    "@tanstack/eslint-plugin-query": "5.74.7",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@types/lodash": "4.14.191",
    "@types/node": "22.15.18",
    "@types/react": "18.2.36",
    "@types/react-dom": "18.2.14",
    "@types/react-swipeable-views": "0.13.6",
    "esbuild": "0.25.3",
    "eslint": "9.26.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-prettier": "5.4.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
